{
  "name": "notes-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "mocha",
    "start": "cross-env DEBUG=notes-app:* USER_SERVICE_URL=http://localhost:3333 node ./app.mjs",
    "start-fs": "cross-env DEBUG=notes:* NOTES_MODEL=fs USER_SERVICE_URL=http://localhost:3333 node ./app.mjs",
    "start-level": "cross-env DEBUG=notes:* NOTES_MODEL=level SEQUELIZE_CONNECT=sequelize.yaml USER_SERVICE_URL=http://localhost:3333 node ./app.mjs",
    "sqlite3-setup": "node ./scripts/init-sqlite3.mjs",
    "start-sqlite3": "cross-env DEBUG=notes:* NOTES_MODEL=sqlite3 USER_SERVICE_URL=http://localhost:3333 node ./app.mjs",
    "start-sequelize": "cross-env NOTES_MODEL=sequelize USER_SERVICE_URL=http://localhost:3333 node ./app.mjs",
    "start-mongodb": "cross-env NOTES_MODEL=mongodb USER_SERVICE_URL=http://localhost:3333 node ./app.mjs",
    "on-server": "cross-env SEQUELIZE_CONNECT=models/sequelize-server-mysql.yaml NOTES_MODEL=sequelize USER_SERVICE_URL=http://localhost:3333 PORT=3000 node ./app.mjs",
    "server1": "set DEBUG=notes-app:* & set NOTES_PORT=3001 & set USER_SERVICE_URL=http://localhost:3333 & node ./app.mjs",
    "server2": "set DEBUG=notes-app:* & set NOTES_PORT=3002 & set USER_SERVICE_URL=http://localhost:3333 & node ./app.mjs",
    "tailwind": "npx @tailwindcss/cli -i ./public/stylesheets/input.css -o ./public/stylesheets/style.css --watch",
    "build-css": "npx @tailwindcss/cli -i ./public/stylesheets/input.css -o ./public/stylesheets/style.css",
    "docker": "node ./app.mjs",
    "docker-build": "docker build -t node-web-development/notes ."
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "express": "^4.22.1",
    "express-session": "^1.18.2",
    "feather-icons": "^4.29.2",
    "fs-extra": "^11.3.2",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "js-yaml": "^4.1.1",
    "level": "^10.0.0",
    "mongodb": "^7.0.0",
    "morgan": "^1.10.1",
    "mysql2": "^3.16.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-local": "^1.0.0",
    "passport.socketio": "^3.7.0",
    "rotating-file-stream": "^3.2.7",
    "sequelize": "^6.37.7",
    "session-file-store": "^1.5.0",
    "socket.io": "^4.8.3",
    "sqlite3": "^5.1.7",
    "superagent": "^10.2.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "chai": "^6.2.2",
    "cross-env": "^10.1.0",
    "mocha": "^11.7.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18"
  }
}
